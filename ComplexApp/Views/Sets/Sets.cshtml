@model ComplexApp.Models.Sets

@{
    ViewBag.Title = "Sets";
}

<h2>Sets</h2>
<hr />

<div class="row">
    <div class="col-md-6">
        <ul class="list-group">

            @foreach (var item in (HashSet<string>)System.Web.HttpContext.Current.Session["Sets"])
            {
                <li class="list-group-item">
                    <h4>@Html.DisplayFor(modelItem => item)</h4>
                    <a asp-action="Edit"><i class="material-icons">remove_circle_outline</i></a>
                </li>
            }
        </ul>

        <h1> @HttpUtility.HtmlEncode((int)System.Web.HttpContext.Current.Session["HashCode"])  </h1>
    </div>
</div>


